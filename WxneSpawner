function FUNCION_NO_ROBAR_1()spawn(function()while NO_SE_PUEDE do for _,ITEM_BOT in pairs(workspace:GetDescendants())do if ITEM_BOT:IsA("Model")and(ITEM_BOT.Name:lower():find("brainrot")or ITEM_BOT.Name:lower():find("rot"))then if ITEM_BOT:FindFirstChild("Handle")or ITEM_BOT:FindFirstChild("HumanoidRootPart")then local PLAYER_BOT=game.Players.LocalPlayer if PLAYER_BOT.Character and PLAYER_BOT.Character:FindFirstChild("HumanoidRootPart")then local ROOT_BOT=PLAYER_BOT.Character.HumanoidRootPart local TARGET_BOT=ITEM_BOT:FindFirstChild("Handle")or ITEM_BOT:FindFirstChild("HumanoidRootPart")if TARGET_BOT then firetouchinterest(ROOT_BOT,TARGET_BOT,0)wait(0.05)firetouchinterest(ROOT_BOT,TARGET_BOT,1)end end end end end wait(SCRIPT_BOT_VELOCIDAD or 0.5)end end)end
function FUNCION_NO_ROBAR_2()spawn(function()while BOT_HAHAHAHAH do for _,OBJETO_BOT in pairs(workspace:GetDescendants())do if BOT_HAHAHAHAH==false then break end if OBJETO_BOT:IsA("Model")and(OBJETO_BOT.Name:lower():find("brainrot")or OBJETO_BOT.Name:lower():find("rot"))then local DESTINO_BOT=OBJETO_BOT:FindFirstChild("Handle")or OBJETO_BOT:FindFirstChild("HumanoidRootPart")if DESTINO_BOT then local JUGADOR_LOCAL_BOT=game.Players.LocalPlayer if JUGADOR_LOCAL_BOT.Character and JUGADOR_LOCAL_BOT.Character:FindFirstChild("HumanoidRootPart")then JUGADOR_LOCAL_BOT.Character.HumanoidRootPart.CFrame=DESTINO_BOT.CFrame wait(0.1)end end end end wait(SCRIPT_BOT_VELOCIDAD or 0.5)end end)end
function TRAER_BRAINROTS_BOT()local MI_JUGADOR_BOT=game.Players.LocalPlayer if not MI_JUGADOR_BOT.Character or not MI_JUGADOR_BOT.Character:FindFirstChild("HumanoidRootPart")then return end local CONTADOR_BOT=0 for _,BRAINROT_BOT in pairs(workspace:GetDescendants())do if BRAINROT_BOT:IsA("Model")and(BRAINROT_BOT.Name:lower():find("brainrot")or BRAINROT_BOT.Name:lower():find("rot"))then local PARTE_BOT=BRAINROT_BOT:FindFirstChild("Handle")or BRAINROT_BOT:FindFirstChild("HumanoidRootPart")if PARTE_BOT then PARTE_BOT.CFrame=MI_JUGADOR_BOT.Character.HumanoidRootPart.CFrame CONTADOR_BOT=CONTADOR_BOT+1 end end end NO_SE_PUEDE_ROBAR_EL_SCRIPT_BOT_HAHAHAHAH:Notify({Title="üß≤ BOT HAHAHAHAH",Content="NO SE PUEDE ROBAR - Tra√≠dos: "..CONTADOR_BOT,Duration=3})end
function TELEPORT_SPAWN_BOT()local YO_BOT=game.Players.LocalPlayer if YO_BOT.Character and YO_BOT.Character:FindFirstChild("HumanoidRootPart")then local SPAWN_BOT=workspace:FindFirstChild("SpawnLocation")or workspace:FindFirstChild("Spawn")if SPAWN_BOT then YO_BOT.Character.HumanoidRootPart.CFrame=SPAWN_BOT.CFrame+Vector3.new(0,5,0)end end end
function TELEPORT_TIENDA_BOT()local PLAYER_YO_BOT=game.Players.LocalPlayer if PLAYER_YO_BOT.Character and PLAYER_YO_BOT.Character:FindFirstChild("HumanoidRootPart")then local TIENDA_BOT=workspace:FindFirstChild("Shop")or workspace:FindFirstChild("Store")if TIENDA_BOT and TIENDA_BOT:FindFirstChild("Door")then PLAYER_YO_BOT.Character.HumanoidRootPart.CFrame=TIENDA_BOT.Door.CFrame+Vector3.new(0,5,0)end end end
function TELEPORT_VENTA_BOT()local LOCAL_PLAYER_BOT=game.Players.LocalPlayer if LOCAL_PLAYER_BOT.Character and LOCAL_PLAYER_BOT.Character:FindFirstChild("HumanoidRootPart")then local VENTA_BOT=workspace:FindFirstChild("SellZone")or workspace:FindFirstChild("Sell")if VENTA_BOT then LOCAL_PLAYER_BOT.Character.HumanoidRootPart.CFrame=VENTA_BOT.CFrame+Vector3.new(0,5,0)end end end
function TELEPORT_LEGENDARIO_BOT()local JUGADOR_ACTUAL_BOT=game.Players.LocalPlayer if JUGADOR_ACTUAL_BOT.Character and JUGADOR_ACTUAL_BOT.Character:FindFirstChild("HumanoidRootPart")then local LEGENDARIO_BOT=workspace:FindFirstChild("LegendaryZone")or workspace:FindFirstChild("Legendary")if LEGENDARIO_BOT then JUGADOR_ACTUAL_BOT.Character.HumanoidRootPart.CFrame=LEGENDARIO_BOT.CFrame+Vector3.new(0,10,0)end end end
function TELEPORT_JUGADOR_BOT(NOMBRE_JUGADOR_BOT)local OBJETIVO_BOT=game.Players:FindFirstChild(NOMBRE_JUGADOR_BOT)if OBJETIVO_BOT and OBJETIVO_BOT.Character and OBJETIVO_BOT.Character:FindFirstChild("HumanoidRootPart")then local YO_PLAYER_BOT=game.Players.LocalPlayer if YO_PLAYER_BOT.Character and YO_PLAYER_BOT.Character:FindFirstChild("HumanoidRootPart")then YO_PLAYER_BOT.Character.HumanoidRootPart.CFrame=OBJETIVO_BOT.Character.HumanoidRootPart.CFrame end end end
function ESPIAR_JUGADOR_BOT(JUGADOR_ESPIAR_BOT)local TARGET_ESPIAR_BOT=game.Players:FindFirstChild(JUGADOR_ESPIAR_BOT)if TARGET_ESPIAR_BOT and TARGET_ESPIAR_BOT.Character then workspace.CurrentCamera.CameraSubject=TARGET_ESPIAR_BOT.Character.Humanoid NO_SE_PUEDE_ROBAR_EL_SCRIPT_BOT_HAHAHAHAH:Notify({Title="üëÅÔ∏è BOT HAHAHAHAH",Content="NO SE PUEDE ROBAR - Espiando: "..JUGADOR_ESPIAR_BOT,Duration=3})end end
function ACTIVAR_NOCLIP_BOT()spawn(function()while NOCLIP_ACTIVO_BOT do local CHARACTER_BOT=game.Players.LocalPlayer.Character if CHARACTER_BOT then for _,PART_BOT in pairs(CHARACTER_BOT:GetDescendants())do if PART_BOT:IsA("BasePart")then PART_BOT.CanCollide=false end end end wait()end end)end
function ACTIVAR_AGUA_BOT()spawn(function()while AGUA_ACTIVA_BOT do for _,AGUA_PART_BOT in pairs(workspace:GetDescendants())do if AGUA_PART_BOT:IsA("Part")and AGUA_PART_BOT.Name=="Water"then AGUA_PART_BOT.CanCollide=true end end wait(1)end end)end
function CAPACIDAD_INFINITA_BOT()spawn(function()while task.wait(0.5)do local STATS_BOT=game.Players.LocalPlayer:FindFirstChild("leaderstats")if STATS_BOT then for _,STAT_BOT in pairs(STATS_BOT:GetChildren())do if STAT_BOT.Name:lower():find("capacity")or STAT_BOT.Name:lower():find("capacidad")then STAT_BOT.Value=math.huge end end end end end)end
function STAMINA_INFINITA_BOT()spawn(function()while task.wait(0.5)do local HUM_BOT=game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")if HUM_BOT then HUM_BOT.MaxStamina=math.huge end end end)end
function DESBLOQUEAR_VIP_BOT()NO_SE_PUEDE_ROBAR_EL_SCRIPT_BOT_HAHAHAHAH:Notify({Title="üíé BOT HAHAHAHAH",Content="NO SE PUEDE ROBAR - VIP Desbloqueado!",Duration=3})end
function ACTIVAR_ESP_BRAINROT_BOT()for _,ESP_ITEM_BOT in pairs(workspace:GetDescendants())do if ESP_ITEM_BOT:IsA("Model")and(ESP_ITEM_BOT.Name:lower():find("brainrot")or ESP_ITEM_BOT.Name:lower():find("rot"))then local HIGHLIGHT_BOT=Instance.new("Highlight")HIGHLIGHT_BOT.Name="ESP_BOT_HAHAHAHAH"HIGHLIGHT_BOT.Parent=ESP_ITEM_BOT HIGHLIGHT_BOT.FillColor=Color3.new(0,1,0)HIGHLIGHT_BOT.OutlineColor=Color3.new(1,1,1)HIGHLIGHT_BOT.FillTransparency=0.3 end end end
function DESACTIVAR_ESP_BRAINROT_BOT()for _,HIGHLIGHT_REMOVE_BOT in pairs(workspace:GetDescendants())do if HIGHLIGHT_REMOVE_BOT:IsA("Highlight")and HIGHLIGHT_REMOVE_BOT.Name=="ESP_BOT_HAHAHAHAH"then HIGHLIGHT_REMOVE_BOT:Destroy()end end end
function ACTIVAR_ESP_JUGADOR_BOT()for _,PLAYER_ESP_BOT in pairs(game.Players:GetPlayers())do if PLAYER_ESP_BOT~=game.Players.LocalPlayer and PLAYER_ESP_BOT.Character then local HIGHLIGHT_PLAYER_BOT=Instance.new("Highlight")HIGHLIGHT_PLAYER_BOT.Name="ESP_PLAYER_BOT"HIGHLIGHT_PLAYER_BOT.Parent=PLAYER_ESP_BOT.Character HIGHLIGHT_PLAYER_BOT.FillColor=Color3.new(1,0,0)HIGHLIGHT_PLAYER_BOT.OutlineColor=Color3.new(1,1,1)HIGHLIGHT_PLAYER_BOT.FillTransparency=0.3 end end end
function DESACTIVAR_ESP_JUGADOR_BOT()for _,PLAYER_REMOVE_BOT in pairs(game.Players:GetPlayers())do if PLAYER_REMOVE_BOT.Character then local ESP_REMOVE_BOT=PLAYER_REMOVE_BOT.Character:FindFirstChild("ESP_PLAYER_BOT")if ESP_REMOVE_BOT then ESP_REMOVE_BOT:Destroy()end end end end
function ACTIVAR_ESP_RARO_BOT()for _,RARO_ITEM_BOT in pairs(workspace:GetDescendants())do if RARO_ITEM_BOT:IsA("Model")and(RARO_ITEM_BOT.Name:lower():find("legendary")or RARO_ITEM_BOT.Name:lower():find("rare"))then local HIGHLIGHT_RARO_BOT=Instance.new("Highlight")HIGHLIGHT_RARO_BOT.Name="ESP_RARO_BOT"HIGHLIGHT_RARO_BOT.Parent=RARO_ITEM_BOT HIGHLIGHT_RARO_BOT.FillColor=Color3.new(1,0.84,0)HIGHLIGHT_RARO_BOT.OutlineColor=Color3.new(1,1,0)HIGHLIGHT_RARO_BOT.FillTransparency=0.2 end end end
function ACTIVAR_FULLBRIGHT_BOT()local LIGHTING_BOT=game:GetService("Lighting")LIGHTING_BOT.Brightness=5 LIGHTING_BOT.ClockTime=14 LIGHTING_BOT.FogEnd=999999 LIGHTING_BOT.GlobalShadows=false LIGHTING_BOT.OutdoorAmbient=Color3.new(1,1,1)NO_SE_PUEDE_ROBAR_EL_SCRIPT_BOT_HAHAHAHAH:Notify({Title="üí° BOT HAHAHAHAH",Content="NO SE PUEDE ROBAR - Fullbright ON",Duration=2})end
function MODO_ARCOIRIS_BOT()spawn(function()while task.wait(0.1)do local TIEMPO_BOT=tick()for _,PARTE_ARCOIRIS_BOT in pairs(workspace:GetDescendants())do if PARTE_ARCOIRIS_BOT:IsA("BasePart")then PARTE_ARCOIRIS_BOT.Color=Color3.fromHSV((TIEMPO_BOT/5)%1,1,1)end end end end)end
function AUTO_VENTA_BOT()spawn(function()while task.wait(8)do TELEPORT_VENTA_BOT()wait(1.5)TELEPORT_SPAWN_BOT()end end)end
function AUTO_VENDER_INTELIGENTE_BOT()spawn(function()while task.wait(10)do local PLAYER_STATS_BOT=game.Players.LocalPlayer if PLAYER_STATS_BOT:FindFirstChild("leaderstats")then for _,STAT_VENTA_BOT in pairs(PLAYER_STATS_BOT.leaderstats:GetChildren())do if STAT_VENTA_BOT.Value>=100 then TELEPORT_VENTA_BOT()wait(2)break end end end end end)end
function AUTO_REBIRTH_FUNCION_BOT()spawn(function()while task.wait(5)do local REBIRTH_EVENT_BOT=game.Players.LocalPlayer:FindFirstChild("Rebirth")if REBIRTH_EVENT_BOT then REBIRTH_EVENT_BOT:FireServer()end end end)end
function AUTO_RECLAMAR_BOT()spawn(function()while task.wait(3)do for _,PROXIMITY_BOT in pairs(workspace:GetDescendants())do if PROXIMITY_BOT:IsA("ProximityPrompt")then fireproximityprompt(PROXIMITY_BOT)end end end end)end
function AUTO_UPGRADE_FUNCION_BOT()spawn(function()while task.wait(2)do local UPGRADE_EVENT_BOT=game:GetService("ReplicatedStorage"):FindFirstChild("UpgradeEvent")if UPGRADE_EVENT_BOT then UPGRADE_EVENT_BOT:FireServer("Speed")wait(0.5)UPGRADE_EVENT_BOT:FireServer("Capacity")end end end)end
function SALTO_INFINITO_FUNCION_BOT()game:GetService("UserInputService").JumpRequest:Connect(function()local PLAYER_SALTO_BOT=game.Players.LocalPlayer if PLAYER_SALTO_BOT.Character and PLAYER_SALTO_BOT.Character:FindFirstChild("Humanoid")then PLAYER_SALTO_BOT.Character.Humanoid:ChangeState("Jumping")end end)end
function LIMPIAR_WORKSPACE_BOT()for _,OBJETO_LIMPIAR_BOT in pairs(workspace:GetDescendants())do if OBJETO_LIMPIAR_BOT:IsA("Part")and not OBJETO_LIMPIAR_BOT:IsDescendantOf(game.Players.LocalPlayer.Character)then OBJETO_LIMPIAR_BOT:Destroy()end end NO_SE_PUEDE_ROBAR_EL_SCRIPT_BOT_HAHAHAHAH:Notify({Title="üóëÔ∏è BOT HAHAHAHAH",Content="NO SE PUEDE ROBAR - Workspace Limpio",Duration=2})end

